<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="org.shaolin.vogerp.productmodel.be.ProductCodeImpl" table="PROD_PRODUCTCODE">
    <id name="id" column="ID">
      <generator class="native" />
    </id>
    <property name="productId" column="PRODUCTID"/>
    <property name="code" column="CODE"/>
    <property name="typeInt" type="integer" column="TYPE"/>
    <property name="_enable" type="boolean" column="_enable"/>
  </class>
</hibernate-mapping>
