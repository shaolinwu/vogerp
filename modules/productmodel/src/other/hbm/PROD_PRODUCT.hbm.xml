<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="org.shaolin.vogerp.productmodel.be.ProductImpl" table="PROD_PRODUCT">
    <id name="id" column="ID">
      <generator class="native" />
    </id>
    <property name="parentId" column="PARENTID"/>
    <property name="name" column="NAME"/>
    <property name="serialNumber" column="SERIALNUMBER"/>
    <property name="onSaleDate" type="date" column="ONSALEDATE"/>
    <property name="saleEndDate" type="date" column="SALEENDDATE"/>
    <property name="description" column="DESCRIPTION"/>
    <property name="photos" column="PHOTOS"/>
    <property name="type" column="TYPE"/>
    <property name="interestedPartyTypeInt" type="integer" column="INTERESTEDPARTYTYPE"/>
    <property name="_enable" type="boolean" column="_enable"/>
  </class>
</hibernate-mapping>
