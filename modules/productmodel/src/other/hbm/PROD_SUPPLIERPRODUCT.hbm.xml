<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="org.shaolin.vogerp.productmodel.be.SupplierProductImpl" table="PROD_SUPPLIERPRODUCT">
    <id name="id" column="ID">
      <generator class="native" />
    </id>
    <property name="productId" column="PRODUCTID"/>
    <property name="description" column="DESCRIPTION"/>
    <property name="feasibleStartDate" type="date" column="FEASIBLESTARTDATE"/>
    <property name="standardDays" column="STANDARDDAYS"/>
    <property name="priorityTypeInt" type="integer" column="PRIORITYTYPE"/>
    <property name="levelTypeInt" type="integer" column="LEVELTYPE"/>
    <property name="_enable" type="boolean" column="_enable"/>
  </class>
</hibernate-mapping>
