  <ns2:eventHandler functionName="analysis">
    <ns2:ops xsi:type="ns2:OpCallAjaxType" name="analysis-20160822-100447">
      <ns2:exp>
        <expressionString><![CDATA[
        import java.util.List;
        import java.util.HashMap;
        import org.shaolin.bmdp.utils.ImageUtil;
        import org.shaolin.uimaster.page.ajax.*;
        import org.shaolin.uimaster.page.AjaxContext;
        import org.shaolin.vogerp.productmodel.internal.PSearcher;
        { 
           if (@page.existElement("productInfoForm")) {
              @page.getRefForm("productInfoForm").remove();
           }
           List list = PSearcher.getInstance().execute(@page.getTextField("searchWordUI").getValue());
           HashMap input = new HashMap();
           input.put("list", list);
           RefForm form = new RefForm("productInfoForm", "org.shaolin.vogerp.productmodel.form.ImportProductsResult", input);
           @page.getPanel("resultPanel").append(form);
        }]]></expressionString>
      </ns2:exp>
    </ns2:ops>
  </ns2:eventHandler>
  <ns2:eventHandler functionName="Cancel">
    <ns2:ops xsi:type="ns2:OpCallAjaxType" name="cancelDetail-20151227-172647">
      <ns2:exp>
        <expressionString><![CDATA[
        import org.shaolin.uimaster.page.AjaxContext;
        import org.shaolin.uimaster.page.ajax.*;
        { 
            RefForm form = (RefForm)@page.getElement(@page.getEntityUiid()); 
            form.closeIfinWindows(true);
            @page.removeForm(@page.getEntityUiid()); 
        }]]></expressionString>
      </ns2:exp>
    </ns2:ops>
  </ns2:eventHandler>
