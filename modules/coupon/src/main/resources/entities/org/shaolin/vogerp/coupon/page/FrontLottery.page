					</ns2:src>
				</ns2:component>
				<ns2:component xsi:type="ns2:UIImageType" UIID="advertRightImage">
					<ns2:src xsi:type="ns2:ImagePropertyType">
						<ns2:location>/coupon/front/advert_right.png</ns2:location>
					</ns2:src>
				</ns2:component>
				<ns2:layoutConstraint>
					<ns2:componentId>lotteryLogoImage</ns2:componentId>
					<ns2:constraint xsi:type="ns2:TableLayoutConstraintType"
						x="0" y="0" align="FULL"></ns2:constraint>
				</ns2:layoutConstraint>
				advertTopImage
