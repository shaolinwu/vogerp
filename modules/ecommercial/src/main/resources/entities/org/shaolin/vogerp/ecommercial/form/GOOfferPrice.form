    <ns2:ComponentMapping xsi:type="ns2:SimpleComponentMappingType" name="simpleMapping1">
      <ns2:mappingRule entityName="org.shaolin.uimaster.page.od.rules.UITextWithCurrency"></ns2:mappingRule>
      <ns2:UIComponent componentPath="currPriceUI" paramName="UIWidgetType"></ns2:UIComponent>
      <ns2:DataComponent xsi:type="ns2:ExpressionParamType" paramName="Currency">
        <ns2:expression>
          <expressionString>{if ($beObject.getEndPrice() > 0) {return $beObject.getEndPrice();} else { return $beObject.getEstimatedPrice();} }</expressionString>
        </ns2:expression>
      </ns2:DataComponent>
    </ns2:ComponentMapping>
    <ns2:ComponentMapping xsi:type="ns2:SimpleComponentMappingType" name="simpleMapping1">
      <ns2:mappingRule entityName="org.shaolin.uimaster.page.od.rules.UITextWithCurrency"></ns2:mappingRule>
      <ns2:UIComponent componentPath="lastPriceUI" paramName="UIWidgetType"></ns2:UIComponent>
      <ns2:DataComponent xsi:type="ns2:ExpressionParamType" paramName="Currency">
        <ns2:expression>
          <expressionString><![CDATA[
          import org.shaolin.vogerp.ecommercial.util.OrderUtil;
          {
            if ($beObject.getEndPrice() > 0) {
               return $beObject.getEndPrice()-1;
            } if ($beObject.getOfferPrices() != null && $beObject.getOfferPrices().size() > 0) {
               return OrderUtil.getLowestOfferPrice($beObject);
            } else { 
               return $beObject.getEstimatedPrice();
            } 
          }]]></expressionString>
        </ns2:expression>
      </ns2:DataComponent>
    </ns2:ComponentMapping>
    <ns2:ComponentMapping xsi:type="ns2:SimpleComponentMappingType" name="simpleMapping2">
      <ns2:mappingRule entityName="org.shaolin.uimaster.page.od.rules.UITextWithCurrency"></ns2:mappingRule>
      <ns2:UIComponent componentPath="priceUI" paramName="UIWidgetType"></ns2:UIComponent>
      <ns2:DataComponent xsi:type="ns2:ExpressionParamType" paramName="Currency">
        <ns2:expression>
          <expressionString><![CDATA[
          import org.shaolin.vogerp.ecommercial.util.OrderUtil;
          {
            if ($beObject.getEndPrice() > 0) {
               return $beObject.getEndPrice()-1;
            } if ($beObject.getOfferPrices() != null && $beObject.getOfferPrices().size() > 0) {
               return OrderUtil.getLowestOfferPrice($beObject) - 1;
            } else { 
               return $beObject.getEstimatedPrice() - 1;
            } 
          }]]></expressionString>
        </ns2:expression>
      </ns2:DataComponent>
    </ns2:ComponentMapping>
