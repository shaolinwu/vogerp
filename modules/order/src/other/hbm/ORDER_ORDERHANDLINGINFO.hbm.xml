<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="org.shaolin.vogerp.order.be.OrderHandlingInfoImpl" table="ORDER_ORDERHANDLINGINFO">
    <id name="id" column="ID">
      <generator class="native" />
    </id>
    <property name="partyId" column="PARTYID"/>
    <property name="hanldingState" column="HANLDINGSTATE"/>
    <property name="percentage" column="PERCENTAGE"/>
    <property name="comment" column="COMMENT"/>
    <property name="_enable" type="boolean" column="_enable"/>
    <property name="_version" type="int" column="_version"/>
    <property name="_starttime" type="timestamp" column="_starttime"/>
    <property name="_endtime" type="timestamp" column="_endtime"/>
    <property name="_optuserid" type="long" column="_optuserid"/>
  </class>
</hibernate-mapping>
