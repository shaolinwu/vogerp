    <ns2:ops xsi:type="ns2:OpCallAjaxType" name="saveDetail1091751003">
      <ns2:exp>
        <expressionString><![CDATA[
        import java.util.List;
        import java.util.HashMap;
        import org.shaolin.uimaster.page.AjaxContext;
        import org.shaolin.uimaster.page.ajax.*;
        import org.shaolin.vogerp.order.be.PurchaseOrderImpl;
        import org.shaolin.vogerp.order.be.PurchaseItemImpl;
        import org.shaolin.vogerp.order.dao.OrderModel;
        import org.shaolin.vogerp.order.util.OrderUtil;
        import org.shaolin.bmdp.runtime.security.UserContext;
        { 
            RefForm form = (RefForm)@page.getElement(@page.getEntityUiid()); 
            HashMap input = (HashMap)form.ui2Data();

            PurchaseOrderImpl defaultUser = (PurchaseOrderImpl)input.get("beObject");
            List items = defaultUser.getItems();
            for (int i=0; i<items.size(); i++) {
                PurchaseItemImpl item = (PurchaseItemImpl)items.get(i);
                item.setTemplate(null);//clear this value.
            }
            if (defaultUser.getId() == 0) {
                defaultUser.setOrgId((Long)UserContext.getUserData(UserContext.CURRENT_USER_ORGID));
                defaultUser.setCreateDate(new java.util.Date());
                defaultUser.setSerialNumber(OrderUtil.genPurchaseOrderSerialNumber());
                OrderModel.INSTANCE.create(defaultUser);
            } else {
                OrderModel.INSTANCE.update(defaultUser);
            }

            form.closeIfinWindows();
            @page.removeForm(@page.getEntityUiid()); 
        }]]></expressionString>
      </ns2:exp>
    </ns2:ops>
    <ns2:ops></ns2:ops>
  </ns2:eventHandler>
