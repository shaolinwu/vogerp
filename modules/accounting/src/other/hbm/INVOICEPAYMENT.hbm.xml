<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="org.shaolin.vogerp.accounting.be.InvoicePaymentImpl" table="INVOICEPAYMENT">
    <property name="id" column="ID"/>
    <property name="serialNumber" column="SERIALNUMBER"/>
    <property name="invoiceId" column="INVOICEID"/>
    <property name="payAmount" column="PAYAMOUNT"/>
    <property name="typeInt" type="integer" column="TYPE"/>
    <property name="_enable" type="boolean" column="_enable"/>
  </class>
</hibernate-mapping>
