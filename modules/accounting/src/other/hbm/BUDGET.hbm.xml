<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="org.shaolin.vogerp.accounting.be.BudgetImpl" table="BUDGET">
    <property name="id" column="ID"/>
    <property name="dateTypeInt" type="integer" column="DATETYPE"/>
    <property name="startDate" type="date" column="STARTDATE"/>
    <property name="endDate" type="date" column="ENDDATE"/>
    <property name="partyType" column="PARTYTYPE"/>
    <property name="isRolling" column="ISROLLING"/>
    <property name="typeInt" type="integer" column="TYPE"/>
    <property name="stateInt" type="integer" column="STATE"/>
    <property name="comments" column="COMMENTS"/>
    <property name="_enable" type="boolean" column="_enable"/>
  </class>
</hibernate-mapping>
