<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="org.shaolin.vogerp.accounting.be.InvoiceItemImpl" table="INVOICEITEM">
    <property name="id" column="ID"/>
    <property name="invoiceId" column="INVOICEID"/>
    <property name="productId" column="PRODUCTID"/>
    <property name="amount" column="AMOUNT"/>
    <property name="unitPrice" column="UNITPRICE"/>
    <property name="typeInt" type="integer" column="TYPE"/>
    <property name="isAdjusted" column="ISADJUSTED"/>
    <property name="statement" column="STATEMENT"/>
    <property name="_enable" type="boolean" column="_enable"/>
  </class>
</hibernate-mapping>
