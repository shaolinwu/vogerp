  <ns2:eventHandler functionName="genVerifiCode">
      <ns2:ops xsi:type="ns2:OpCallAjaxType" name="genVerifiCode-20180127-1839">
          <ns2:exp>
              <expressionString><![CDATA[
          import org.shaolin.uimaster.page.AjaxContext;
          import org.shaolin.uimaster.page.ajax.*;
          import org.shaolin.vogerp.commonmodel.IUserService;
          import org.shaolin.bmdp.runtime.AppContext;
          { 
              IUserService userService = (IUserService)AppContext.get().getService(IUserService.class);
              String question = userService.preLogin(@page.getRequest());
              Label label = @page.getLabel("veriCodeQuestion");
              label.setValue(question);
          }
          ]]></expressionString>
          </ns2:exp>
      </ns2:ops>
  </ns2:eventHandler>
  <ns2:eventHandler functionName="verifiCode">
      <ns2:ops xsi:type="ns2:OpExecuteScriptType">
          <ns2:expressionString><![CDATA[
          {   
              var constraint_result = this.Form.validate();
              if (constraint_result != true && constraint_result != null) {
                  return false;
              }
          }
          ]]></ns2:expressionString>
      </ns2:ops>
      <ns2:ops xsi:type="ns2:OpCallAjaxType" name="verifiCode-20180127-1839">
          <ns2:exp>
              <expressionString><![CDATA[
          import org.shaolin.uimaster.page.AjaxContext;
          import org.shaolin.uimaster.page.ajax.*;
          import org.shaolin.vogerp.commonmodel.IUserService;
          import org.shaolin.bmdp.runtime.AppContext;
          { 
              TextField veriCode = @page.getTextField("veriCode");
              IUserService userService = (IUserService)AppContext.get().getService(IUserService.class);
              if (@page.getRequest().getSession().getAttribute("has_first_token") == null 
                 || userService.checkVerifiAnswer(veriCode.getValue(), @page.getRequest())) {
                 veriCode.addStyle("background-color", "greenyellow");
              } else {
                 veriCode.addStyle("background-image", "none");
                 veriCode.addStyle("background-color", "red");
              }
          }
          ]]></expressionString>
          </ns2:exp>
      </ns2:ops>
  </ns2:eventHandler>
  <ns2:eventHandler functionName="sendPassword">
    <ns2:ops xsi:type="ns2:OpExecuteScriptType">
       <ns2:expressionString><![CDATA[
       {   
           var constraint_result = this.Form.validate();
           if (constraint_result != true && constraint_result != null) {
               return false;
           }
       }
       ]]></ns2:expressionString>
    </ns2:ops>
    <ns2:ops xsi:type="ns2:OpCallAjaxType" name="sendPassword_2040990210">
      <ns2:exp>
        <expressionString><![CDATA[
        import java.util.List;
        import org.shaolin.uimaster.page.AjaxContext;
        import org.shaolin.bmdp.runtime.AppContext;
        import org.shaolin.uimaster.page.ajax.*;
		import org.shaolin.bmdp.runtime.ce.ConstantServiceImpl;
        import org.shaolin.vogerp.commonmodel.util.CEOperationUtil;
        import org.shaolin.vogerp.commonmodel.IUserService;
        import org.shaolin.bmdp.runtime.ce.ConstantServiceImpl;
        import org.shaolin.bmdp.runtime.spi.IAppServiceManager;
        { 
           String phoneNumber = @page.getTextField("phoneNumberUI").getValue();
           IUserService msgService = (IUserService)AppContext.get().getService(IUserService.class);
           msgService.resetPassword(@page.getRequest().getSession(), phoneNumber);
           Dialog.showMessageDialog("已发送密码到您手机，请注意查看。", "提醒", Dialog.INFORMATION_MESSAGE, null);
        }
        ]]></expressionString>
      </ns2:exp>
    </ns2:ops>
  </ns2:eventHandler>
  <ns2:eventHandler functionName="Cancel">
    <ns2:ops xsi:type="ns2:OpCallAjaxType" name="cancelDetail1358686533">
      <ns2:exp>
        <expressionString><![CDATA[
        import org.shaolin.uimaster.page.AjaxContext;
        import org.shaolin.uimaster.page.ajax.*;
        { 
            RefForm form = (RefForm)@page.getElement(@page.getEntityUiid()); 
            form.closeIfinWindows(true);
            @page.removeForm(@page.getEntityUiid()); 
        }]]></expressionString>
      </ns2:exp>
    </ns2:ops>
  </ns2:eventHandler>
