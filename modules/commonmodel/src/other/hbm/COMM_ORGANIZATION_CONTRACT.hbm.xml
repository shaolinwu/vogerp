<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="org.shaolin.vogerp.commonmodel.be.OrganizationContractImpl" table="COMM_ORGANIZATION_CONTRACT">
    <id name="id" column="ID">
      <generator class="native" />
    </id>
    <property name="orgId" column="ORGID"/>
    <property name="typeInt" type="integer" column="TYPE"/>
    <property name="terms" column="TERMS"/>
    <property name="expirationDate" type="date" column="EXPIRATIONDATE"/>
    <property name="_enable" type="boolean" column="_enable"/>
  </class>
</hibernate-mapping>
