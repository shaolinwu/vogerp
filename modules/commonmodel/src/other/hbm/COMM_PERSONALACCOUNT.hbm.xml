<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="org.shaolin.vogerp.commonmodel.be.PersonalAccountImpl" table="COMM_PERSONALACCOUNT">
    <id name="id" column="ID">
      <generator class="native" />
    </id>
    <property name="userName" column="USERNAME"/>
    <property name="password" column="PASSWORD"/>
    <property name="locale" column="LOCALE"/>
    <property name="attempt" column="ATTEMPT"/>
    <property name="isLocked" column="ISLOCKED"/>
    <property name="lockedMoment" type="date" column="LOCKEDMOMENT"/>
    <property name="expirationDate" type="date" column="EXPIRATIONDATE"/>
    <property name="lastLogin" type="date" column="LASTLOGIN"/>
    <many-to-one name="info" column="PERSONALID" unique="true" not-null="true" lazy="false" insert="false" update="false"/>
    <property name="_enable" type="boolean" column="_enable"/>
    <property name="_version" type="int" column="_version"/>
    <property name="_starttime" type="timestamp" column="_starttime"/>
    <property name="_endtime" type="timestamp" column="_endtime"/>
    <property name="_optuserid" type="long" column="_optuserid"/>
  </class>
</hibernate-mapping>
