<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="org.shaolin.vogerp.commonmodel.be.UIWidgetPermissionImpl" table="COMM_UIWIDGETPERMISSION">
    <id name="id" column="ID">
      <generator class="native" />
    </id>
    <property name="pageName" column="PAGENAME"/>
    <property name="widgetId" column="WIDGETID"/>
    <property name="partyType" column="PARTYTYPE"/>
    <property name="_enable" type="boolean" column="_enable"/>
  </class>
</hibernate-mapping>
