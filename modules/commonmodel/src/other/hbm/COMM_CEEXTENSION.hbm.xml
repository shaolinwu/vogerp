<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="org.shaolin.vogerp.commonmodel.be.CEExtensionImpl" table="COMM_CEEXTENSION">
    <id name="id" column="ID">
      <generator class="native" />
    </id>
    <property name="ceName" column="CENAME"/>
    <property name="intValue" column="INTVALUE"/>
    <property name="stringValue" column="STRINGVALUE"/>
    <property name="i18nKey" column="I18NKEY"/>
    <property name="description" column="DESCRIPTION"/>
    <property name="icon" column="ICON"/>
    <property name="_enable" type="boolean" column="_enable"/>
  </class>
</hibernate-mapping>
