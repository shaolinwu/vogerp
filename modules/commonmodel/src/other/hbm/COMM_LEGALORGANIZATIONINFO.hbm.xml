<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="org.shaolin.vogerp.commonmodel.be.LegalOrganizationInfoImpl" table="COMM_LEGALORGANIZATIONINFO">
    <id name="id" column="ID">
      <generator class="native" />
    </id>
    <property name="orgId" column="ORGID"/>
    <property name="legalPerson" column="LEGALPERSON"/>
    <property name="tradeLicenseNumber" column="TRADELICENSENUMBER"/>
    <property name="busiLicenseNumber" column="BUSILICENSENUMBER"/>
    <property name="busiLicenseDate" type="date" column="BUSILICENSEDATE"/>
    <property name="taxRegistrationNumber" column="TAXREGISTRATIONNUMBER"/>
    <property name="taxRegistrationDate" type="date" column="TAXREGISTRATIONDATE"/>
    <property name="localTaxRegistrationNumber" column="LOCALTAXREGISTRATIONNUMBER"/>
    <property name="localTaxRegistrationDate" type="date" column="LOCALTAXREGISTRATIONDATE"/>
    <property name="enterpriseCode" column="ENTERPRISECODE"/>
    <property name="bankAccount" column="BANKACCOUNT"/>
    <property name="bankTypeInt" type="integer" column="BANKTYPE"/>
    <property name="_enable" type="boolean" column="_enable"/>
  </class>
</hibernate-mapping>
