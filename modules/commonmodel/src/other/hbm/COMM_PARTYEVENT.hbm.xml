<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="org.shaolin.vogerp.commonmodel.be.PartyEventImpl" table="COMM_PARTYEVENT">
    <id name="id" column="ID">
      <generator class="native" />
    </id>
    <property name="srcPartyId" column="SRCPARTYID"/>
    <property name="tarPartyId" column="TARPARTYID"/>
    <property name="pageInfo" column="PAGEINFO"/>
    <property name="eventRecord" column="EVENTRECORD"/>
    <property name="eventTypeInt" type="integer" column="EVENTTYPE"/>
    <property name="eventStateInt" type="integer" column="EVENTSTATE"/>
    <property name="_enable" type="boolean" column="_enable"/>
    <property name="_version" type="int" column="_version"/>
    <property name="_starttime" type="timestamp" column="_starttime"/>
    <property name="_endtime" type="timestamp" column="_endtime"/>
    <property name="_optuserid" type="long" column="_optuserid"/>
  </class>
</hibernate-mapping>
