<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="org.shaolin.vogerp.commonmodel.be.OrganizationImpl" table="COMM_ORGANIZATION">
    <id name="id" column="ID">
      <generator class="native" />
    </id>
    <property name="parentId" column="PARENTID"/>
    <property name="orgCode" column="ORGCODE"/>
    <property name="name" column="NAME"/>
    <property name="description" column="DESCRIPTION"/>
    <property name="type" column="TYPE"/>
    <property name="_enable" type="boolean" column="_enable"/>
  </class>
</hibernate-mapping>
