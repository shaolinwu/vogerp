  <ns2:eventHandler functionName="Save">
    <ns2:ops xsi:type="ns2:OpCallAjaxType" name="saveDetail1988586262">
      <ns2:exp>
        <expressionString><![CDATA[
        import java.util.HashMap;
        import java.util.List;
        import org.shaolin.bmdp.runtime.AppContext;
        import org.shaolin.uimaster.page.AjaxContext;
        import org.shaolin.uimaster.page.ajax.*;
        import org.shaolin.vogerp.commonmodel.be.CEExtensionImpl;
        import org.shaolin.vogerp.commonmodel.dao.ModularityModel;
        import org.shaolin.vogerp.commonmodel.util.CEOperationUtil;
        { 
            CEExtensionImpl defaultUser = new CEExtensionImpl();
            HashMap input = new HashMap();
            input.put("beObject", defaultUser);
            input.put("editable", new Boolean(true));
            RefForm form = (RefForm)@page.getElement(@page.getEntityUiid()); 
            form.ui2Data(input);

            defaultUser = (CEExtensionImpl)input.get("beObject");
            if (defaultUser.getId() == 0) {
               List result = ModularityModel.INSTANCE.searchCEExtension(defaultUser, null, 0, -1);
               if (result.size() == 0) {
				               ModularityModel.INSTANCE.create(defaultUser);
				               AppContext.get().getConstantService().addConstantItem(CEOperationUtil.convertSingD2C(defaultUser));
               } else {
                   CEExtensionImpl newRecord = (CEExtensionImpl)result.get(0);
                   newRecord.setStringValue(defaultUser.getStringValue());
																	  newRecord.setDescription(defaultUser.getDescription());
																	  newRecord.setI18nKey(defaultUser.getI18nKey());
                   ModularityModel.INSTANCE.update(newRecord);
                   AppContext.get().getConstantService().updateConstantItem(CEOperationUtil.convertSingD2C(newRecord));
               }
            } else {
               ModularityModel.INSTANCE.update(defaultUser);
               AppContext.get().getConstantService().updateConstantItem(CEOperationUtil.convertSingD2C(defaultUser));
            }
            
            form.closeIfinWindows();
            @page.removeForm(@page.getEntityUiid()); 
        }]]></expressionString>
      </ns2:exp>
    </ns2:ops>
    <ns2:ops></ns2:ops>
  </ns2:eventHandler>
  <ns2:eventHandler functionName="selectImage">
    <ns2:ops xsi:type="ns2:OpCallAjaxType" name="selectImage-1317628417">
      <ns2:exp>
        <expressionString><![CDATA[
        import java.util.HashMap;
        import org.shaolin.uimaster.page.AjaxContext;
        import org.shaolin.uimaster.page.ajax.*;
        { 
           HashMap input = new HashMap();
           input.put("editable", new Boolean(true));
           input.put("callbackUIID", @page.getTextField("iconUI").getId());
           RefForm form = new RefForm("selectImageForm", "org.shaolin.vogerp.commonmodel.form.ImageViewer", input);
           @page.addElement(form);
           form.openInWindows("Image Viewer", null, 620, 450);
        }]]></expressionString>
      </ns2:exp>
    </ns2:ops>
  </ns2:eventHandler>
  <ns2:eventHandler functionName="Cancel">
    <ns2:ops xsi:type="ns2:OpCallAjaxType" name="cancelDetail1317628417">
      <ns2:exp>
        <expressionString><![CDATA[
        import org.shaolin.uimaster.page.AjaxContext;
        import org.shaolin.uimaster.page.ajax.*;
        { 
            RefForm form = (RefForm)@page.getElement(@page.getEntityUiid()); 
            form.closeIfinWindows(true);
            @page.removeForm(@page.getEntityUiid()); 
        }]]></expressionString>
      </ns2:exp>
    </ns2:ops>
  </ns2:eventHandler>
