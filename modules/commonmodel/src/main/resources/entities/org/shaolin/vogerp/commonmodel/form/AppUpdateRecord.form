    <ns2:ops xsi:type="ns2:OpCallAjaxType" name="saveDetail-20170207-173332">
      <ns2:exp>
        <expressionString><![CDATA[
        import java.util.HashMap;
        import org.shaolin.uimaster.page.AjaxContext;
        import org.shaolin.uimaster.page.ajax.*;
        import org.shaolin.vogerp.commonmodel.be.AppUpdateRecordImpl;
        { 
            RefForm form = (RefForm)@page.getElement(@page.getEntityUiid()); 
            HashMap out = (HashMap)form.ui2Data();

            AppUpdateRecordImpl defaultUser = (AppUpdateRecordImpl)out.get("beObject");
            if (defaultUser.getId() == 0) {
                System.out.println("created object: " + defaultUser);
            } else {
                System.out.println("updated object: " + defaultUser);
            }

            form.closeIfinWindows();
            @page.removeForm(@page.getEntityUiid()); 
            Dialog.showMessageDialog("更新成功！", "", Dialog.INFORMATION_MESSAGE, null);
        }]]></expressionString>
      </ns2:exp>
    </ns2:ops>
  </ns2:eventHandler>
