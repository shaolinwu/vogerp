  <ns2:eventHandler functionName="setAsDefaultAddress">
    <ns2:ops xsi:type="ns2:OpCallAjaxType" name="setAsDefaultAddress-20151229-1148">
      <ns2:exp>
        <expressionString><![CDATA[
        import java.util.List;
        import java.util.HashMap;
        import org.shaolin.uimaster.page.AjaxContext;
        import org.shaolin.uimaster.page.ajax.*;
        import org.shaolin.vogerp.commonmodel.be.AddressInfoImpl;
        import org.shaolin.vogerp.commonmodel.dao.ModularityModel;
        { 
            Table tableWidget = (Table)@page.getElement("itemTable");
            if (tableWidget.getSelectedRow() == null) {
                return;
            }
            AddressInfoImpl defaultAddress = null;
            List rows = tableWidget.getListData();
            for (int i=0; i<rows.size(); i++) {
                AddressInfoImpl item = (AddressInfoImpl)rows.get(i);
                if (item.getDefaultAddress()) {
                   defaultAddress = item;
                   break;
                }
            }
            if (defaultAddress != null) {
                defaultAddress.setDefaultAddress(false);
                ModularityModel.INSTANCE.update(defaultAddress);
            }
            AddressInfoImpl address = (AddressInfoImpl)tableWidget.getSelectedRow();
            address.setDefaultAddress(true);
            ModularityModel.INSTANCE.update(address);
            
            tableWidget.refresh();
            Dialog.showMessageDialog("设置成功！", "", Dialog.INFORMATION_MESSAGE, null);
        }]]></expressionString>
      </ns2:exp>
    </ns2:ops>
  </ns2:eventHandler>
