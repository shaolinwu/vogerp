    <ns2:ops xsi:type="ns2:OpCallAjaxType" name="saveDetail-2117220046">
      <ns2:exp>
        <expressionString><![CDATA[
        import java.util.HashMap;
        import org.shaolin.bmdp.runtime.AppContext;
        import org.shaolin.uimaster.page.AjaxContext;
        import org.shaolin.uimaster.page.ajax.*;
		import org.shaolin.bmdp.runtime.security.UserContext;
	    import org.shaolin.vogerp.commonmodel.IUserService;
        import org.shaolin.vogerp.commonmodel.ce.EmployeeLevel;
        import org.shaolin.vogerp.commonmodel.be.IPersonalInfo;
        import org.shaolin.vogerp.commonmodel.be.PersonalInfoImpl;
        import org.shaolin.vogerp.commonmodel.be.PersonalAccountImpl;
        import org.shaolin.vogerp.commonmodel.dao.CommonModel;
        { 
		    UserContext userContext = UserContext.getUserContext();
		    IUserService userService = (IUserService)AppContext.get().getService(IUserService.class);
		    PersonalAccountImpl personalAccount = (PersonalAccountImpl)userService.getPersonalAccount(userContext.getUserId());
            HashMap input = new HashMap();
            input.put("beObject", personalAccount.getInfo());
            RefForm form = (RefForm)@page.getElement(@page.getEntityUiid()); 
            HashMap output = (HashMap)form.ui2Data(input);
            PersonalInfoImpl defaultUser = (PersonalInfoImpl)output.get("beObject");
            if (defaultUser.getId() == 0) {
                defaultUser.setOrgId(org.shaolin.bmdp.runtime.security.UserContext.getUserContext().getOrgId());
                defaultUser.setEmpLevel(EmployeeLevel.JUNIOR);
                CommonModel.INSTANCE.create(defaultUser);
            } else {
                CommonModel.INSTANCE.update(defaultUser);
            }
            userService.updateUserCache((IPersonalInfo)defaultUser);
            
            if (form != null && form.isInWindows()) {
                form.closeIfinWindows();
                @page.removeForm(@page.getEntityUiid());
            }
            Dialog.showMessageDialog("更新成功！", "", Dialog.INFORMATION_MESSAGE, null);
        }]]></expressionString>
      </ns2:exp>
    </ns2:ops>
  </ns2:eventHandler>
