  <ns2:eventHandler functionName="createItem">
    <ns2:ops xsi:type="ns2:OpCallAjaxType" name="createItem_1993740592">
      <ns2:exp>
        <expressionString><![CDATA[
        import java.util.HashMap;
        import org.shaolin.uimaster.page.AjaxContext;
        import org.shaolin.uimaster.page.ajax.*;
        import org.shaolin.vogerp.commonmodel.be.ContactInfoImpl;
        { 
           String pid = @page.getHidden("parentIdUI").getValue();
           ContactInfoImpl contact = new ContactInfoImpl();    
           
           HashMap input = new HashMap();
           input.put("beObject", contact);
           input.put("parentId", Long.valueOf(pid));
           input.put("editable", new Boolean(true));
           RefForm form = new RefForm("contactForm", "org.shaolin.vogerp.commonmodel.form.ContactInfo", input);
           @page.addElement(form);
           
           form.openInWindows("新增联系人信息", null);
        }]]></expressionString>
      </ns2:exp>
    </ns2:ops>
  </ns2:eventHandler>
  <ns2:eventHandler functionName="openItem">
    <ns2:ops xsi:type="ns2:OpCallAjaxType" name="openItem-1993740592">
      <ns2:exp>
        <expressionString><![CDATA[
        import java.util.HashMap;
        import org.shaolin.uimaster.page.AjaxContext;
        import org.shaolin.uimaster.page.ajax.*;
        import org.shaolin.vogerp.commonmodel.be.ContactInfoImpl;
        { 
           String pid = @page.getHidden("parentIdUI").getValue();
           Table tableWidget = (Table)@page.getElement("itemTable");
           if (tableWidget.getSelectedRow() == null) {
              return;
           }
           ContactInfoImpl contact = (ContactInfoImpl)tableWidget.getSelectedRow();
           
           HashMap input = new HashMap();
           input.put("beObject", contact);
           input.put("parentId", Long.valueOf(pid));
           input.put("editable", new Boolean(true));
           RefForm form = new RefForm("contactForm", "org.shaolin.vogerp.commonmodel.form.ContactInfo", input);
           @page.addElement(form);
           
           form.openInWindows("打开联系人信息", new TableCallBack("itemTable"));
        }]]></expressionString>
      </ns2:exp>
    </ns2:ops>
  </ns2:eventHandler>
