                     </ns2:expression>
                </ns2:value>
           </ns2:Property>
           <ns2:Property name="editable">
               <ns2:value xsi:type="ns2:ExpressionPropertyType">
                     <ns2:expression>
                       <expressionString><![CDATA[
                       { 
                           return $editable; 
                       }]]></expressionString>
                     </ns2:expression>
                </ns2:value>
           </ns2:Property>
           <ns2:editable xsi:type="ns2:ExpressionPropertyType">
