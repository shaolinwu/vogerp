    <ns2:constructorCode><![CDATA[
       var othis = this;
       this.uploadFile.callback = function() {
           othis.refresh(othis.uploadFile);
       }
       this.uploadFile.cleanAll = function() {
           othis.clean(othis.uploadFile);
       }
    ]]>
    </ns2:constructorCode>
